<?xml version="1.0"?>
<launch>
  <node pkg="camera1394" type="camera1394_node" name="left_node" ns="left">
    
 <!--   <param name="camera_info_url" value="file:///$(find cob_camera_sensors)/ros/launch/left_cob3-1_640x480_rgb8_tof.yaml" /> -->
    <param name="frame_id" value="head_color_camera_l_link" />
    <param name="auto_white_balance" value="2" />
    <param name="auto_brightness" value="2" />
    <param name="auto_exposure" value="2" />
    <param name="auto_gain" value="2" />
    <param name="auto_gamma" value="2" />
    <param name="auto_hue" value="2" />
    <param name="auto_iris" value="2" />
 <!--   <param name="auto_saturation" value="2" /> -->
    <param name="auto_shutter" value="2" />
    <param name="guid" value="000a470110073080" />
    <param name="video_mode" value="format7_mode0" />
    <param name="format7_color_coding" value="rgb8" />
    <param name="reset_on_open" value="true" />

   <!-- <param name="video_mode" value="640x480_rgb8" /> -->
    <!--param name="video_mode" value="1280x960_rgb8" /-->
    <param name="iso_speed" value="400" />
 <!--   <param name="frame_rate" value="15" /> -->
    <!--param name="frame_rate" value="5" /-->
    <remap from="camera/image_raw" to="image_raw"/>
    <remap from="camera/camera_info" to="camera_info"/>
  </node>
  <node ns="left" pkg="image_proc" type="image_proc" respawn="false" name="left_image_proc" />
</launch>

